<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center"
    android:orientation="vertical"
    android:id="@+id/sheetDeleteMultipleDialog"
    android:tag="layout/dialog_deletemember_migrate_0"
    android:padding="@dimen/grid_2"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/AssignCollagueTxt"
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/grid_1"
        app:endIconMode="custom"
        app:hintAnimationEnabled="false"
        app:hintEnabled="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:startIconDrawable="0x7f080458"
        style="@style/Superfone.Input.WithoutLine">
        <androidx.appcompat.widget.AppCompatAutoCompleteTextView
            android:textColor="?attr/colorOnSurface"
            android:id="@+id/mAutoCompleteColleague"
            android:background="@drawable/board_selected"
            android:paddingTop="0dp"
            android:paddingBottom="0dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/search_by_colleague"
            android:maxLines="1"
            android:singleLine="true"
            android:completionThreshold="2"
            android:inputType="textNoSuggestions"
            android:imeOptions="actionDone"
            style="@style/Widget.AppCompat.Light.AutoCompleteTextView"/>
    </com.google.android.material.textfield.TextInputLayout>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/mACClose"
        android:visibility="gone"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:src="@drawable/ic_search_close"
        android:paddingEnd="@dimen/grid_2"
        app:layout_constraintBottom_toBottomOf="@+id/AssignCollagueTxt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/AssignCollagueTxt"/>
    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonCancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/grid_2"
        android:text="@string/cancel"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        app:backgroundTint="@color/superfone_gray_300"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AssignCollagueTxt"
        style="@style/Widget.Superfone.TextButtonNoPadding"/>
    <com.google.android.material.button.MaterialButton
        android:textColor="?attr/colorOnPrimary"
        android:id="@+id/buttonDone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/grid_2"
        android:text="@string/confirm"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        app:backgroundTint="?attr/colorPrimary"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AssignCollagueTxt"
        style="@style/Widget.Superfone.TextButtonNoPadding"/>
</androidx.constraintlayout.widget.ConstraintLayout>
