<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:tag="layout/dialog_fragment_container_0"
    android:padding="0dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="0dp"
    android:minWidth="@dimen/grid_15">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:background="@color/superfone_purple"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize">
        <com.google.android.material.textview.MaterialTextView
            android:textAppearance="@style/TextAppearance.Superfone.Headline4"
            android:textStyle="bold"
            android:textColor="?attr/colorOnPrimary"
            android:gravity="start"
            android:id="@+id/leadTitle"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Alert"
            android:maxLines="1"
            android:layout_marginStart="@dimen/grid_2"
            android:layout_marginEnd="@dimen/grid_2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/closeDialog"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/closeDialog"
            android:padding="@dimen/grid_1_5"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:tint="@color/superfone_white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="H, 1:1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_cross"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/existingLeadBlock"
        android:tag="binding_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:maxHeight="@dimen/grid_10"
        android:minWidth="@dimen/grid_15">
        <com.google.android.material.textview.MaterialTextView
            android:textAppearance="@style/TextAppearance.Superfone.Headline5"
            android:textColor="?attr/colorOnSurface"
            android:id="@+id/tvDate"
            android:tag="binding_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_0_5"
            android:layout_marginStart="@dimen/grid_2"
            android:layout_marginEnd="@dimen/grid_1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <FrameLayout
            android:id="@+id/frame_container_ivContactProfilePicInLead"
            android:layout_width="@dimen/grid_5"
            android:layout_height="56dp"
            android:layout_marginTop="@dimen/grid_0_5"
            android:layout_marginStart="@dimen/grid_1_5"
            app:layout_behavior="@string/appbar_scrolling_view_behavior"
            app:layout_constraintDimensionRatio="H, 1:1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvDate">
            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/ivContactProfilePicLead"
                android:tag="binding_3"
                android:padding="@dimen/grid_0_5"
                android:layout_width="@dimen/grid_5"
                android:layout_height="@dimen/grid_5"
                android:scaleType="centerInside"
                app:shapeAppearanceOverlay="@style/ShapeAppearance.Superfone.RoundedImageView"/>
            <TextView
                android:textColor="?attr/colorOnPrimary"
                android:gravity="center"
                android:layout_gravity="end|top"
                android:id="@+id/activityFilter_badge"
                android:tag="binding_4"
                android:background="@drawable/badge_background"
                android:padding="3dp"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginTop="@dimen/grid_0_5"
                android:alpha="0.5"
                android:layout_marginEnd="@dimen/grid_0_5"/>
            <com.google.android.material.textview.MaterialTextView
                android:textSize="8sp"
                android:textStyle="bold"
                android:textColor="?attr/colorOnPrimary"
                android:gravity="center"
                android:layout_gravity="bottom|center_horizontal"
                android:id="@+id/leadLifecycle"
                android:tag="binding_5"
                android:background="@color/superfone_black_30"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/grid_1"
                android:text="Fresh"
                android:maxLines="1"
                android:paddingStart="@dimen/grid_0_5"
                android:paddingEnd="@dimen/grid_0_5"
                app:layout_constraintEnd_toEndOf="@+id/tvContactName"
                app:layout_constraintStart_toEndOf="@+id/frame_container_ivContactProfilePicInLead"
                app:layout_constraintTop_toBottomOf="@+id/tvContactNumber"/>
        </FrameLayout>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/ivConnectedSource"
            android:tag="binding_6"
            android:layout_width="@dimen/grid_3"
            android:layout_height="@dimen/grid_3"
            android:scaleType="centerInside"
            android:layout_marginStart="@dimen/grid_0_5"
            app:layout_constraintBottom_toBottomOf="@+id/tvContactName"
            app:layout_constraintStart_toEndOf="@+id/frame_container_ivContactProfilePicInLead"
            app:layout_constraintTop_toTopOf="@+id/tvContactName"/>
        <com.google.android.material.textview.MaterialTextView
            android:textAppearance="@style/TextAppearance.Superfone.Headline5"
            android:id="@+id/tvContactName"
            android:tag="binding_7"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_1_5"
            android:layout_marginStart="@dimen/grid_0_5"
            android:layout_marginEnd="@dimen/grid_1"
            app:layout_constraintEnd_toStartOf="@+id/leadOwnerName"
            app:layout_constraintStart_toEndOf="@+id/ivConnectedSource"
            app:layout_constraintTop_toBottomOf="@+id/tvDate"
            app:layout_constraintVertical_chainStyle="packed"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/hasBusinessWebsite"
            android:tag="binding_8"
            android:layout_width="@dimen/grid_2"
            android:layout_height="@dimen/grid_2"
            android:src="@drawable/ic_web"
            android:scaleType="centerInside"
            android:tint="@color/superfone_green"
            android:layout_marginStart="@dimen/grid_0_5"
            app:layout_constraintBottom_toBottomOf="@+id/tvContactNumber"
            app:layout_constraintStart_toEndOf="@+id/frame_container_ivContactProfilePicInLead"
            app:layout_constraintTop_toTopOf="@+id/tvContactNumber"/>
        <com.google.android.material.textview.MaterialTextView
            android:textAppearance="@style/TextAppearance.Superfone.Headline6"
            android:textColor="?attr/colorOnSurface"
            android:id="@+id/tvContactNumber"
            android:tag="binding_9"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_0_5"
            android:layout_marginStart="@dimen/grid_0_5"
            app:layout_constraintEnd_toStartOf="@+id/callTimeBlock"
            app:layout_constraintStart_toEndOf="@+id/hasBusinessWebsite"
            app:layout_constraintTop_toBottomOf="@+id/tvContactName"/>
        <com.google.android.material.chip.Chip
            android:textAppearance="@style/TextAppearance.Superfone.Subtitle2"
            android:id="@+id/leadOwnerName"
            android:tag="binding_10"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginTop="@dimen/grid_1_5"
            android:layout_marginStart="@dimen/grid_1"
            android:layout_marginEnd="@dimen/grid_1"
            app:chipBackgroundColor="?attr/colorSurface"
            app:chipIconSize="@dimen/grid_2"
            app:ensureMinTouchTargetSize="false"
            app:iconEndPadding="2dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tvDate"
            app:textEndPadding="0dp"
            app:textStartPadding="2dp"/>
        <LinearLayout
            android:gravity="center"
            android:orientation="horizontal"
            android:id="@+id/callTimeBlock"
            android:visibility="visible"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_0_5"
            android:layout_marginEnd="@dimen/grid_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/leadOwnerName">
            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/callTypeIcon"
                android:tag="binding_11"
                android:layout_width="@dimen/grid_1_5"
                android:layout_height="@dimen/grid_1_5"
                android:layout_marginStart="@dimen/grid_0_5"
                android:layout_marginEnd="@dimen/grid_0_5"
                app:layout_constraintBottom_toTopOf="@+id/actionGroup"
                app:layout_constraintEnd_toStartOf="@+id/callTime"/>
            <com.google.android.material.textview.MaterialTextView
                android:textAppearance="@style/TextAppearance.Superfone.Body5"
                android:textColor="?attr/colorOnSurface"
                android:id="@+id/callAttempts"
                android:tag="binding_12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/grid_0_5"/>
            <com.google.android.material.textview.MaterialTextView
                android:textAppearance="@style/TextAppearance.Superfone.Body5"
                android:textColor="?attr/colorOnSurface"
                android:id="@+id/callTime"
                android:tag="binding_13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:tag="binding_14"
        android:padding="0dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="0dp"
        android:minWidth="@dimen/grid_15">
        <androidx.cardview.widget.CardView
            android:id="@+id/itemLeadRootCardView"
            android:tag="binding_15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/grid_1"
            android:paddingEnd="@dimen/grid_1"
            app:cardBackgroundColor="#d9fdd3"
            app:cardCornerRadius="12dp"
            app:cardElevation="0dp"
            app:cardPreventCornerOverlap="false"
            app:cardUseCompatPadding="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintWidth_max="wrap">
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/layoutMessageWithImage"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/reply_layout"
                    android:tag="binding_16"
                    android:background="?attr/colorSurface"
                    android:padding="@dimen/grid_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <View
                        android:id="@+id/image_line"
                        android:background="@color/superfone_green"
                        android:layout_width="2dp"
                        android:layout_height="0dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/textQuotedMessageSentBy"
                        android:tag="binding_17"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginHorizontal="8dp"
                        app:layout_constraintStart_toStartOf="@+id/image_line"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <com.google.android.material.textview.MaterialTextView
                        android:textSize="14sp"
                        android:textColor="?attr/colorOnSurface"
                        android:ellipsize="end"
                        android:id="@+id/textQuotedMessage"
                        android:tag="binding_18"
                        android:paddingTop="@dimen/grid_0_5"
                        android:paddingBottom="@dimen/grid_0_5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:maxLines="2"
                        android:paddingStart="@dimen/grid_0_5"
                        android:paddingEnd="@dimen/grid_1"
                        android:layout_marginHorizontal="8dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="@+id/image_line"
                        app:layout_constraintTop_toBottomOf="@+id/textQuotedMessageSentBy"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/ivFileView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="?attr/colorSurface"
                    app:cardCornerRadius="@dimen/grid_1_5"
                    app:cardElevation="0dp"
                    app:cardMaxElevation="0dp"
                    app:strokeColor="@color/backgroundTasks"
                    app:strokeWidth="0.5dp">
                    <LinearLayout
                        android:orientation="vertical"
                        android:tag="binding_19"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <RelativeLayout
                            android:layout_gravity="center"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <com.google.android.material.imageview.ShapeableImageView
                                android:id="@+id/ivFilePreview"
                                android:tag="binding_20"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:scaleType="fitCenter"
                                android:adjustViewBounds="true"
                                app:shapeAppearanceOverlay="@style/RoundedSquareTopOnly"/>
                            <com.google.android.material.imageview.ShapeableImageView
                                android:id="@+id/ivPlayButton"
                                android:tag="binding_21"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/grid_8"
                                android:src="@drawable/ic_play_filled"
                                android:scaleType="centerInside"
                                android:adjustViewBounds="true"
                                android:tint="@color/superfone_black_30"
                                android:scaleX="2"
                                android:scaleY="2"
                                app:contentPaddingBottom="0dp"
                                app:shapeAppearanceOverlay="@style/RoundedSquare"/>
                        </RelativeLayout>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:id="@+id/ivFileDetail"
                            android:tag="binding_22"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/grid_1_5"
                            android:layout_marginEnd="@dimen/grid_1_5">
                            <com.google.android.material.card.MaterialCardView
                                android:id="@+id/ivFileTypeIcon"
                                android:tag="binding_23"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/grid_4"
                                android:layout_marginTop="@dimen/grid_1_5"
                                android:layout_marginBottom="@dimen/grid_1_5"
                                android:minWidth="@dimen/grid_4"
                                app:cardBackgroundColor="@color/backgroundVisits20"
                                app:cardCornerRadius="2dp"
                                app:cardElevation="0dp"
                                app:cardMaxElevation="0dp">
                                <com.google.android.material.imageview.ShapeableImageView
                                    android:layout_gravity="center"
                                    android:id="@+id/ivFileTypeIconDrawable"
                                    android:tag="binding_24"
                                    android:layout_width="@dimen/grid_3"
                                    android:layout_height="@dimen/grid_3"
                                    android:src="@drawable/ic_file_open"
                                    android:scaleType="centerInside"
                                    android:adjustViewBounds="true"
                                    android:tint="?attr/colorPrimary"
                                    app:contentPaddingBottom="0dp"
                                    app:shapeAppearanceOverlay="@style/RoundedSquare"/>
                            </com.google.android.material.card.MaterialCardView>
                            <com.google.android.material.textview.MaterialTextView
                                android:textAppearance="@style/TextAppearance.Superfone.Body3"
                                android:ellipsize="end"
                                android:layout_gravity="center"
                                android:id="@+id/ivFileNameText"
                                android:tag="binding_25"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:maxLines="1"
                                android:layout_marginStart="@dimen/grid_1"/>
                            <ProgressBar
                                android:textAppearance="@style/TextAppearance.Superfone.Body3"
                                android:layout_gravity="center"
                                android:tag="binding_26"
                                android:layout_width="@dimen/grid_3"
                                android:layout_height="@dimen/grid_3"
                                android:layout_marginStart="@dimen/grid_1"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:tag="binding_27"
                        android:padding="8dp"
                        android:layout_width="@dimen/grid_10"
                        android:layout_height="wrap_content">
                        <ImageButton
                            android:id="@+id/btnPlayPause"
                            android:tag="binding_28"
                            android:background="?attr/selectableItemBackgroundBorderless"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_play"/>
                        <SeekBar
                            android:id="@+id/seekBar"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:maxHeight="4dp"
                            android:progressDrawable="@drawable/seekbar_progress"
                            android:minHeight="4dp"
                            android:thumb="@drawable/seekbar_thumb"
                            android:layout_weight="1"/>
                        <TextView
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:id="@+id/tvDuration"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="00:00"
                            android:paddingStart="8dp"/>
                    </LinearLayout>
                    <com.google.android.material.imageview.ShapeableImageView
                        android:layout_gravity="center"
                        android:id="@+id/ivLocationStaticMap"
                        android:tag="binding_29"
                        android:layout_width="@dimen/grid_15"
                        android:layout_height="@dimen/grid_15"
                        android:scaleType="centerCrop"
                        android:adjustViewBounds="true"
                        app:shapeAppearanceOverlay="@style/RoundedSquare"/>
                </com.google.android.material.card.MaterialCardView>
                <View
                    android:tag="binding_30"
                    android:background="@drawable/ic_bottom_border"
                    android:visibility="invisible"
                    android:layout_width="@dimen/grid_10"
                    android:layout_height="0.1dp"/>
                <com.google.android.material.imageview.ShapeableImageView
                    android:layout_gravity="center"
                    android:id="@+id/ivContactProfilePic"
                    android:tag="binding_31"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:scaleType="fitCenter"
                    android:adjustViewBounds="true"
                    app:shapeAppearanceOverlay="@style/RoundedSquare"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/superfone_black"
                    android:autoLink="web"
                    android:id="@+id/textMessage"
                    android:tag="binding_32"
                    android:paddingLeft="12dp"
                    android:paddingTop="@dimen/grid_1"
                    android:paddingRight="12dp"
                    android:paddingBottom="@dimen/grid_1"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="1"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="?attr/colorOnSurface"
                    android:ellipsize="end"
                    android:id="@+id/textQuotedMessage_hidden"
                    android:tag="binding_33"
                    android:padding="@dimen/grid_0_5"
                    android:visibility="invisible"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:maxLines="2"
                    android:layout_marginEnd="@dimen/grid_3"
                    android:layout_marginHorizontal="8dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/image_line"
                    app:layout_constraintTop_toBottomOf="@+id/textQuotedMessageSentBy"/>
                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/textQuotedMessageSentBy_hidden"
                    android:tag="binding_34"
                    android:visibility="invisible"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_marginEnd="@dimen/grid_3"
                    android:layout_marginHorizontal="8dp"
                    app:layout_constraintStart_toStartOf="@+id/image_line"
                    app:layout_constraintTop_toBottomOf="@+id/textQuotedMessage_hidden"/>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/btnInteractive"
                        android:tag="binding_35"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/grid_1"
                        android:layout_marginEnd="@dimen/grid_1"/>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:id="@+id/oneButtonContainer"
                        android:tag="binding_36"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/grid_2"
                        android:layout_marginEnd="@dimen/grid_2">
                        <Button
                            android:textSize="8sp"
                            android:id="@+id/button1SingleButton"
                            android:tag="binding_37"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            style="@style/Widget.Material3.Button.OutlinedButton"/>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:id="@+id/twoButtonsContainer"
                        android:tag="binding_38"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/grid_2"
                        android:layout_marginEnd="@dimen/grid_2">
                        <Button
                            android:textSize="8sp"
                            android:id="@+id/button1TwoButtons"
                            android:tag="binding_39"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_marginEnd="@dimen/grid_0_5"
                            style="@style/Widget.Material3.Button.OutlinedButton"/>
                        <Button
                            android:textSize="8sp"
                            android:id="@+id/button2TwoButtons"
                            android:tag="binding_40"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_marginStart="@dimen/grid_0_5"
                            style="@style/Widget.Material3.Button.OutlinedButton"/>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:id="@+id/threeButtonsContainer"
                        android:tag="binding_41"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/grid_2"
                        android:layout_marginEnd="@dimen/grid_2">
                        <LinearLayout
                            android:orientation="horizontal"
                            android:id="@+id/threeButtonsContainerFirstPart"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <Button
                                android:textSize="8sp"
                                android:id="@+id/button1"
                                android:tag="binding_42"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:layout_marginEnd="@dimen/grid_0_5"
                                style="@style/Widget.Material3.Button.OutlinedButton"/>
                            <Button
                                android:textSize="8sp"
                                android:id="@+id/button2"
                                android:tag="binding_43"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:layout_marginStart="@dimen/grid_0_5"
                                style="@style/Widget.Material3.Button.OutlinedButton"/>
                        </LinearLayout>
                        <Button
                            android:textSize="8sp"
                            android:id="@+id/button3"
                            android:tag="binding_44"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            style="@style/Widget.Material3.Button.OutlinedButton"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </androidx.cardview.widget.CardView>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/quickActionBar"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.google.android.material.button.MaterialButton
            android:id="@+id/newLeadWhatsappButton"
            android:visibility="visible"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="WA"
            android:layout_weight="1"
            android:layout_marginStart="@dimen/grid_2"
            android:layout_marginEnd="@dimen/grid_2"
            android:backgroundTint="@color/superfone_green"
            app:icon="@drawable/ic_whatsapp"/>
        <com.google.android.material.button.MaterialButton
            android:id="@+id/callButton"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Call"
            android:layout_weight="1"
            android:textAllCaps="false"
            android:layout_marginStart="@dimen/grid_2"
            android:layout_marginEnd="@dimen/grid_2"
            app:icon="@drawable/ic_call"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/dismissAllOptionBar"
        android:padding="0dp"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="0dp">
        <com.google.android.material.button.MaterialButton
            android:id="@+id/dimissAllButton"
            android:padding="0dp"
            android:visibility="visible"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:text="Dismiss All"
            android:layout_weight="1"
            android:backgroundTint="@color/superfone_red_70"
            app:cornerRadius="0dp"/>
        <com.google.android.material.button.MaterialButton
            android:id="@+id/viewNextButton"
            android:padding="0dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:text="View Next"
            android:layout_weight="1"
            android:textAllCaps="false"
            android:backgroundTint="@color/colorOnSurface"
            app:cornerRadius="0dp"/>
    </LinearLayout>
</LinearLayout>
