<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/content_home_progress_0"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/grid_1">
    <ai.salesmax.view.ViewProgressCard2
        android:id="@+id/progressOne"
        android:tag="binding_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/progressTwo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ai.salesmax.view.ViewProgressCard2
        android:id="@+id/progressTwo"
        android:tag="binding_2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/progressOne"
        app:layout_constraintTop_toTopOf="parent"/>
    <ai.salesmax.view.ViewProgressCard2
        android:id="@+id/progressThree"
        android:tag="binding_3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/progressTwo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/progressOne"/>
    <ai.salesmax.view.ViewProgressCard2
        android:id="@+id/progressFour"
        android:tag="binding_4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/progressThree"
        app:layout_constraintTop_toTopOf="@+id/progressThree"/>
</androidx.constraintlayout.widget.ConstraintLayout>
