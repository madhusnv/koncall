<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/itemLeadRoot"
    android:tag="layout/item_lead_0"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="1dp"
    android:animateLayoutChanges="true"
    app:cardBackgroundColor="?attr/colorSurface"
    app:cardCornerRadius="0dp"
    app:materialThemeOverlay="@style/ThemeOverlay.Superfone.ElevationOverlayDisabled"
    app:strokeWidth="0dp"
    style="?attr/materialCardViewElevatedStyle">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:paddingBottom="@dimen/grid_1_5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.google.android.material.textview.MaterialTextView
            android:textAppearance="@style/TextAppearance.Superfone.Headline5"
            android:textColor="?attr/colorOnSurface"
            android:id="@+id/tvDate"
            android:tag="binding_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_0_5"
            android:layout_marginStart="@dimen/grid_2"
            android:layout_marginEnd="@dimen/grid_1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <FrameLayout
            android:id="@+id/frame_container_ivContactProfilePicInLead"
            android:layout_width="@dimen/grid_5"
            android:layout_height="56dp"
            android:layout_marginTop="@dimen/grid_0_5"
            android:layout_marginStart="@dimen/grid_1_5"
            app:layout_behavior="@string/appbar_scrolling_view_behavior"
            app:layout_constraintDimensionRatio="H, 1:1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvDate">
            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/ivContactProfilePic"
                android:tag="binding_2"
                android:padding="@dimen/grid_0_5"
                android:layout_width="@dimen/grid_5"
                android:layout_height="@dimen/grid_5"
                android:scaleType="centerInside"
                app:shapeAppearanceOverlay="@style/ShapeAppearance.Superfone.RoundedImageView"/>
            <TextView
                android:textColor="?attr/colorOnPrimary"
                android:gravity="center"
                android:layout_gravity="end|top"
                android:id="@+id/activityFilter_badge"
                android:tag="binding_3"
                android:background="@drawable/badge_background"
                android:padding="3dp"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginTop="@dimen/grid_0_5"
                android:alpha="0.5"
                android:layout_marginEnd="@dimen/grid_0_5"/>
            <com.google.android.material.textview.MaterialTextView
                android:textSize="8sp"
                android:textStyle="bold"
                android:textColor="?attr/colorOnPrimary"
                android:gravity="center"
                android:layout_gravity="bottom|center_horizontal"
                android:id="@+id/leadLifecycle"
                android:tag="binding_4"
                android:background="@color/superfone_black_70"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/grid_1"
                android:text="Fresh"
                android:maxLines="1"
                android:paddingStart="@dimen/grid_0_5"
                android:paddingEnd="@dimen/grid_0_5"
                app:layout_constraintEnd_toEndOf="@+id/tvContactName"
                app:layout_constraintStart_toEndOf="@+id/frame_container_ivContactProfilePicInLead"
                app:layout_constraintTop_toBottomOf="@+id/tvContactNumber"/>
        </FrameLayout>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/ivConnectedSource"
            android:tag="binding_5"
            android:layout_width="@dimen/grid_3"
            android:layout_height="@dimen/grid_3"
            android:scaleType="centerInside"
            android:layout_marginStart="@dimen/grid_0_5"
            app:layout_constraintBottom_toBottomOf="@+id/tvContactName"
            app:layout_constraintStart_toEndOf="@+id/frame_container_ivContactProfilePicInLead"
            app:layout_constraintTop_toTopOf="@+id/tvContactName"/>
        <com.google.android.material.textview.MaterialTextView
            android:textAppearance="@style/TextAppearance.Superfone.Headline5"
            android:id="@+id/tvContactName"
            android:tag="binding_6"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_1_5"
            android:layout_marginStart="@dimen/grid_0_5"
            android:layout_marginEnd="@dimen/grid_1"
            app:layout_constraintEnd_toStartOf="@+id/leadOwnerName"
            app:layout_constraintStart_toEndOf="@+id/ivConnectedSource"
            app:layout_constraintTop_toBottomOf="@+id/tvDate"
            app:layout_constraintVertical_chainStyle="packed"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/hasBusinessWebsite"
            android:tag="binding_7"
            android:layout_width="@dimen/grid_2"
            android:layout_height="@dimen/grid_2"
            android:src="@drawable/ic_web"
            android:scaleType="centerInside"
            android:tint="@color/superfone_green"
            android:layout_marginStart="@dimen/grid_0_5"
            app:layout_constraintBottom_toBottomOf="@+id/tvContactNumber"
            app:layout_constraintStart_toEndOf="@+id/frame_container_ivContactProfilePicInLead"
            app:layout_constraintTop_toTopOf="@+id/tvContactNumber"/>
        <com.google.android.material.textview.MaterialTextView
            android:textAppearance="@style/TextAppearance.Superfone.Body5"
            android:textColor="?attr/colorOnSurface"
            android:id="@+id/tvContactNumber"
            android:tag="binding_8"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_0_5"
            android:layout_marginStart="@dimen/grid_0_5"
            app:layout_constraintEnd_toStartOf="@+id/callTimeBlock"
            app:layout_constraintStart_toEndOf="@+id/hasBusinessWebsite"
            app:layout_constraintTop_toBottomOf="@+id/tvContactName"/>
        <com.google.android.material.card.MaterialCardView
            android:id="@+id/recentActionBlock"
            android:tag="binding_9"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_0_5"
            android:layout_marginStart="@dimen/grid_0_5"
            android:layout_marginEnd="@dimen/grid_1_5"
            android:backgroundTint="@color/colorBackground"
            android:outlineProvider="none"
            app:cardCornerRadius="@dimen/grid_1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/frame_container_ivContactProfilePicInLead"
            app:layout_constraintTop_toBottomOf="@+id/tvContactNumber"
            app:strokeWidth="0dp">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:tag="binding_10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.google.android.material.textview.MaterialTextView
                    android:textAppearance="@style/TextAppearance.Superfone.Body5"
                    android:textStyle="bold"
                    android:textColor="?attr/colorOnSurface"
                    android:gravity="start"
                    android:id="@+id/tvLeadActiveDealStage"
                    android:tag="binding_11"
                    android:padding="@dimen/grid_0_5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/grid_0_5"
                    android:layout_marginEnd="@dimen/grid_1"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/recentActions"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <include
                    android:id="@+id/recentActions"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    layout="@layout/content_recent_actions_linear"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.card.MaterialCardView>
        <com.google.android.material.chip.Chip
            android:textAppearance="@style/TextAppearance.Superfone.Subtitle2"
            android:ellipsize="end"
            android:id="@+id/leadOwnerName"
            android:tag="binding_12"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:maxWidth="@dimen/grid_9"
            android:maxLines="1"
            android:layout_marginStart="@dimen/grid_1"
            android:layout_marginEnd="@dimen/grid_1"
            app:chipBackgroundColor="@color/colorPrimaryContainer"
            app:chipIconSize="@dimen/grid_2"
            app:chipStrokeWidth="0dp"
            app:ensureMinTouchTargetSize="false"
            app:iconEndPadding="2dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tvContactName"
            app:textEndPadding="0dp"
            app:textStartPadding="2dp"/>
        <LinearLayout
            android:gravity="center"
            android:orientation="horizontal"
            android:id="@+id/callTimeBlock"
            android:visibility="visible"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_0_5"
            android:layout_marginEnd="@dimen/grid_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/leadOwnerName">
            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/callTypeIcon"
                android:tag="binding_13"
                android:layout_width="@dimen/grid_1_5"
                android:layout_height="@dimen/grid_1_5"
                android:layout_marginStart="@dimen/grid_0_5"
                android:layout_marginEnd="@dimen/grid_0_5"
                app:layout_constraintBottom_toTopOf="@+id/actionGroup"
                app:layout_constraintEnd_toStartOf="@+id/callTime"/>
            <com.google.android.material.textview.MaterialTextView
                android:textAppearance="@style/TextAppearance.Superfone.Body5"
                android:textColor="?attr/colorOnSurface"
                android:id="@+id/callAttempts"
                android:tag="binding_14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/grid_0_5"/>
            <com.google.android.material.textview.MaterialTextView
                android:textAppearance="@style/TextAppearance.Superfone.Body5"
                android:textColor="?attr/colorOnSurface"
                android:id="@+id/callTime"
                android:tag="binding_15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <com.google.android.material.textview.MaterialTextView
                android:textAppearance="@style/TextAppearance.Superfone.Body5"
                android:textColor="?attr/colorOnSurface"
                android:id="@+id/createTime"
                android:tag="binding_16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <CheckBox
                android:id="@+id/itemCheckBox"
                android:tag="binding_17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:minWidth="0dp"
                android:minHeight="0dp"/>
        </LinearLayout>
        <com.google.android.material.card.MaterialCardView
            android:id="@+id/videoMeetingBlock"
            android:tag="binding_18"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_0_5"
            android:layout_marginStart="@dimen/grid_0_5"
            android:layout_marginEnd="@dimen/grid_1_5"
            android:backgroundTint="@color/superfone_purple_10"
            android:outlineProvider="none"
            app:cardCornerRadius="@dimen/grid_1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/frame_container_ivContactProfilePicInLead"
            app:layout_constraintTop_toBottomOf="@+id/recentActionBlock"
            app:strokeWidth="0dp">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/ivVideoPlatformPick"
                    android:tag="binding_19"
                    android:padding="@dimen/grid_0_5"
                    android:layout_width="@dimen/grid_3"
                    android:layout_height="@dimen/grid_3"
                    android:layout_marginTop="@dimen/grid_0_5"
                    android:scaleType="centerInside"
                    android:layout_marginStart="@dimen/grid_1_5"
                    app:layout_constraintDimensionRatio="H, 1:1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:shapeAppearanceOverlay="@style/ShapeAppearance.Superfone.RoundedImageView"
                    app:srcCompat="@drawable/google_meet_icon"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textAppearance="@style/TextAppearance.Superfone.Headline6"
                    android:textColor="?attr/colorOnSurface"
                    android:id="@+id/videoMeetTime"
                    android:tag="binding_20"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/grid_1"
                    android:layout_marginEnd="@dimen/grid_1"
                    app:layout_constraintBottom_toBottomOf="@+id/ivVideoPlatformPick"
                    app:layout_constraintEnd_toStartOf="@+id/btnJoinMeeting"
                    app:layout_constraintStart_toEndOf="@+id/ivVideoPlatformPick"
                    app:layout_constraintTop_toTopOf="@+id/ivVideoPlatformPick"/>
                <Button
                    android:textSize="9sp"
                    android:id="@+id/btnJoinMeeting"
                    android:tag="binding_21"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:minWidth="0dp"
                    android:minHeight="0dp"
                    android:text="Join"
                    app:layout_constraintBottom_toBottomOf="@+id/ivVideoPlatformPick"
                    app:layout_constraintEnd_toStartOf="@+id/btnUpdateMeetingStatus"
                    app:layout_constraintTop_toTopOf="@+id/ivVideoPlatformPick"
                    style="@style/Widget.Material3.Button.TextButton"/>
                <Button
                    android:textSize="9sp"
                    android:id="@+id/btnUpdateMeetingStatus"
                    android:tag="binding_22"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:minWidth="0dp"
                    android:minHeight="0dp"
                    android:text="Done?"
                    app:layout_constraintBottom_toBottomOf="@+id/ivVideoPlatformPick"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/ivVideoPlatformPick"
                    style="@style/Widget.Material3.Button.TextButton"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.card.MaterialCardView>
        <com.google.android.material.card.MaterialCardView
            android:id="@+id/paymentPendingBlock"
            android:tag="binding_23"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_0_5"
            android:layout_marginStart="@dimen/grid_0_5"
            android:layout_marginEnd="@dimen/grid_1_5"
            android:backgroundTint="@color/superfone_purple_10"
            android:outlineProvider="none"
            app:cardCornerRadius="@dimen/grid_1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/frame_container_ivContactProfilePicInLead"
            app:layout_constraintTop_toBottomOf="@+id/videoMeetingBlock"
            app:strokeWidth="0dp">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/ivPaymentMethod"
                    android:tag="binding_24"
                    android:layout_width="@dimen/grid_4"
                    android:layout_height="@dimen/grid_4"
                    android:scaleType="centerInside"
                    android:tint="?attr/colorPrimary"
                    android:layout_marginStart="@dimen/grid_0_5"
                    app:layout_constraintDimensionRatio="H, 1:1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:shapeAppearanceOverlay="@style/ShapeAppearance.Superfone.RoundedImageView"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textAppearance="@style/TextAppearance.Superfone.Headline6"
                    android:textColor="?attr/colorOnSurface"
                    android:id="@+id/paymentAmount"
                    android:tag="binding_25"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/grid_0_5"
                    app:layout_constraintBottom_toBottomOf="@+id/ivPaymentMethod"
                    app:layout_constraintEnd_toStartOf="@+id/paymentRequestedAt"
                    app:layout_constraintStart_toEndOf="@+id/ivPaymentMethod"
                    app:layout_constraintTop_toTopOf="@+id/ivPaymentMethod"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="9sp"
                    android:id="@+id/paymentRequestedAt"
                    android:tag="binding_26"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:minWidth="0dp"
                    android:minHeight="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/ivPaymentMethod"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/ivPaymentMethod"
                    style="@style/Widget.Material3.Button.TextButton"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.card.MaterialCardView>
        <HorizontalScrollView
            android:id="@+id/actionGroup"
            android:tag="binding_27"
            android:scrollbars="none"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/paymentPendingBlock">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingStart="@dimen/grid_6"
                android:paddingEnd="@dimen/grid_1">
                <com.google.android.material.button.MaterialButton
                    android:id="@+id/leadCall"
                    android:tag="binding_28"
                    android:layout_width="@dimen/grid_4"
                    android:layout_height="@dimen/grid_4"
                    app:backgroundTint="?attr/colorPrimary"
                    app:icon="@drawable/ic_call"
                    app:iconTint="?attr/colorOnPrimary"
                    app:layout_constraintEnd_toStartOf="@+id/leadCallCloud"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/Superfone.Button.IconOnly"/>
                <com.google.android.material.button.MaterialButton
                    android:id="@+id/leadCallCloud"
                    android:tag="binding_29"
                    android:layout_width="@dimen/grid_4"
                    android:layout_height="@dimen/grid_4"
                    android:layout_marginStart="@dimen/grid_4"
                    app:backgroundTint="@color/superfone_red_70"
                    app:icon="@drawable/ic_phone_cloud"
                    app:iconTint="?attr/colorOnPrimary"
                    app:layout_constraintEnd_toStartOf="@+id/leadConversation"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintStart_toEndOf="@+id/leadCall"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/Superfone.Button.IconOnly"/>
                <com.google.android.material.button.MaterialButton
                    android:id="@+id/leadConversation"
                    android:tag="binding_30"
                    android:layout_width="@dimen/grid_4"
                    android:layout_height="@dimen/grid_4"
                    android:layout_marginStart="@dimen/grid_4"
                    app:backgroundTint="@color/superfone_purple"
                    app:icon="@drawable/ic_conversation"
                    app:iconTint="?attr/colorOnPrimary"
                    app:layout_constraintEnd_toStartOf="@+id/leadWhatsapp"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintStart_toEndOf="@+id/leadCallCloud"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/Superfone.Button.IconOnly"/>
                <com.google.android.material.button.MaterialButton
                    android:id="@+id/leadWhatsapp"
                    android:tag="binding_31"
                    android:layout_width="@dimen/grid_4"
                    android:layout_height="@dimen/grid_4"
                    android:layout_marginStart="@dimen/grid_4"
                    app:backgroundTint="@android:color/holo_green_dark"
                    app:icon="@drawable/ic_whatsapp"
                    app:iconTint="?attr/colorOnPrimary"
                    app:layout_constraintEnd_toStartOf="@+id/leadVoiceNote"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintStart_toEndOf="@+id/leadConversation"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/Superfone.Button.IconOnly"/>
                <com.google.android.material.button.MaterialButton
                    android:id="@+id/leadVoiceNote"
                    android:tag="binding_32"
                    android:layout_width="@dimen/grid_4"
                    android:layout_height="@dimen/grid_4"
                    android:layout_marginStart="@dimen/grid_4"
                    app:backgroundTint="@color/superfone_red"
                    app:icon="@drawable/ic_microphone"
                    app:iconTint="?attr/colorOnPrimary"
                    app:layout_constraintEnd_toStartOf="@+id/leadAdd"
                    app:layout_constraintStart_toEndOf="@+id/leadWhatsapp"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/Superfone.Button.IconOnly"/>
                <com.google.android.material.button.MaterialButton
                    android:id="@+id/leadAdd"
                    android:tag="binding_33"
                    android:layout_width="@dimen/grid_4"
                    android:layout_height="@dimen/grid_4"
                    android:layout_marginStart="@dimen/grid_4"
                    app:backgroundTint="@android:color/holo_purple"
                    app:icon="@drawable/ic_plus"
                    app:iconTint="?attr/colorOnPrimary"
                    app:layout_constraintEnd_toStartOf="@+id/leadMessage"
                    app:layout_constraintStart_toEndOf="@+id/leadVoiceNote"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/Superfone.Button.IconOnly"/>
                <com.google.android.material.button.MaterialButton
                    android:id="@+id/leadMessage"
                    android:tag="binding_34"
                    android:layout_width="@dimen/grid_4"
                    android:layout_height="@dimen/grid_4"
                    android:layout_marginStart="@dimen/grid_4"
                    app:backgroundTint="@android:color/holo_blue_dark"
                    app:icon="@drawable/ic_comment"
                    app:iconTint="?attr/colorOnPrimary"
                    app:layout_constraintEnd_toStartOf="@+id/leadShare"
                    app:layout_constraintStart_toEndOf="@+id/leadAdd"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/Superfone.Button.IconOnly"/>
                <com.google.android.material.button.MaterialButton
                    android:id="@+id/leadShare"
                    android:tag="binding_35"
                    android:layout_width="@dimen/grid_4"
                    android:layout_height="@dimen/grid_4"
                    android:layout_marginStart="@dimen/grid_4"
                    app:backgroundTint="@android:color/holo_orange_dark"
                    app:icon="@drawable/ic_share"
                    app:iconTint="?attr/colorOnPrimary"
                    app:layout_constraintEnd_toEndOf="@+id/leadInfo"
                    app:layout_constraintStart_toEndOf="@+id/leadMessage"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/Superfone.Button.IconOnly"/>
                <com.google.android.material.button.MaterialButton
                    android:id="@+id/leadInfo"
                    android:tag="binding_36"
                    android:layout_width="@dimen/grid_4"
                    android:layout_height="@dimen/grid_4"
                    android:layout_marginStart="@dimen/grid_4"
                    app:backgroundTint="@android:color/darker_gray"
                    app:icon="@drawable/ic_info"
                    app:iconTint="?attr/colorOnPrimary"
                    app:layout_constraintEnd_toStartOf="@+id/leadEdit"
                    app:layout_constraintStart_toEndOf="@+id/leadShare"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/Superfone.Button.IconOnly"/>
                <com.google.android.material.button.MaterialButton
                    android:id="@+id/leadEdit"
                    android:tag="binding_37"
                    android:layout_width="@dimen/grid_4"
                    android:layout_height="@dimen/grid_4"
                    android:layout_marginStart="@dimen/grid_4"
                    app:backgroundTint="@color/superfone_red"
                    app:icon="@drawable/ic_edit"
                    app:iconTint="?attr/colorOnPrimary"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/leadInfo"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/Superfone.Button.IconOnly"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </HorizontalScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</com.google.android.material.card.MaterialCardView>
