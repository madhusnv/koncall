<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/contact_info_fragment_0"
    android:background="?attr/colorLightBackground"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <com.google.android.material.card.MaterialCardView
        android:tag="binding_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardBackgroundColor="?attr/colorLightBackground"
        app:cardCornerRadius="0dp"
        app:cardElevation="@dimen/grid_1"
        app:cardMaxElevation="@dimen/grid_1">
        <LinearLayout
            android:orientation="vertical"
            android:paddingTop="@dimen/grid_2"
            android:paddingBottom="@dimen/grid_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/grid_2"
            android:paddingEnd="@dimen/grid_2">
            <com.google.android.material.textview.MaterialTextView
                android:textAppearance="@style/TextAppearance.Superfone.Headline5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/grid_1_5"
                android:text="@string/basic_info"/>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:orientation="vertical"
                android:id="@+id/allContact"
                android:tag="binding_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <ai.salesmax.view.ViewProfileData
                android:tag="binding_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/grid_1_5"
                app:superfone_profile_data_icon="@drawable/ic_designation"
                app:superfone_profile_data_title="Designation"/>
            <ai.salesmax.view.ViewProfileData
                android:id="@+id/businessName"
                android:tag="binding_4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:superfone_profile_data_icon="@drawable/ic_company"
                app:superfone_profile_data_title="Company"/>
            <ai.salesmax.view.ViewProfileData
                android:tag="binding_5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:superfone_profile_data_icon="@drawable/ic_email"
                app:superfone_profile_data_title="Email Address"/>
            <ai.salesmax.view.ViewProfileData
                android:tag="binding_6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:superfone_profile_data_icon="@drawable/ic_contact_type"
                app:superfone_profile_data_title="Contact type"/>
            <ai.salesmax.view.ViewProfileData
                android:tag="binding_7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:superfone_profile_data_icon="@drawable/ic_contact_owner"
                app:superfone_profile_data_title="Contact Owner"/>
            <com.google.android.material.textview.MaterialTextView
                android:textAppearance="@style/TextAppearance.Superfone.Headline5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/grid_2"
                android:text="@string/associations"/>
            <ai.salesmax.view.ViewProfileData
                android:tag="binding_8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/grid_1_5"
                app:superfone_profile_data_icon="@drawable/ic_designation"
                app:superfone_profile_data_title="Related Business"/>
            <ai.salesmax.view.ViewProfileData
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/grid_1"
                app:superfone_profile_data_icon="@drawable/ic_company"
                app:superfone_profile_data_title="Related Contacts"
                app:superfone_profile_data_value="None"/>
            <ai.salesmax.view.ViewProfileData
                android:tag="binding_9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/grid_1"
                app:superfone_profile_data_icon="@drawable/ic_contact_type"
                app:superfone_profile_data_title="Related Deals"/>
            <ai.salesmax.view.ViewProfileData
                android:tag="binding_10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/grid_1"
                app:superfone_profile_data_icon="@drawable/ic_location"
                app:superfone_profile_data_title="Location"/>
            <ai.salesmax.view.ViewProfileData
                android:tag="binding_11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/grid_1"
                app:superfone_profile_data_icon="@drawable/ic_location"
                app:superfone_profile_data_title="Street Address"/>
            <ai.salesmax.view.ViewProfileData
                android:tag="binding_12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/grid_1"
                app:superfone_profile_data_icon="@drawable/ic_location"
                app:superfone_profile_data_title="City"/>
            <ai.salesmax.view.ViewProfileData
                android:tag="binding_13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/grid_1"
                app:superfone_profile_data_icon="@drawable/ic_location"
                app:superfone_profile_data_title="State"/>
            <ai.salesmax.view.ViewProfileData
                android:tag="binding_14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/grid_1"
                app:superfone_profile_data_icon="@drawable/ic_location"
                app:superfone_profile_data_title="Country"/>
            <ai.salesmax.view.ViewProfileData
                android:tag="binding_15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/grid_1"
                app:superfone_profile_data_icon="@drawable/ic_location"
                app:superfone_profile_data_title="Pincode"/>
        </LinearLayout>
    </com.google.android.material.card.MaterialCardView>
</androidx.core.widget.NestedScrollView>
