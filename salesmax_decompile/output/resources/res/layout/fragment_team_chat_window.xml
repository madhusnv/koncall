<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/rootView"
    android:tag="layout/fragment_team_chat_window_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/chatDetailTitle"
        android:tag="binding_1"
        android:background="?attr/colorSurface"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/buttonClose"
            android:layout_width="@dimen/grid_4"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_back_arrow"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/ivContactProfilePic"
            android:tag="binding_2"
            android:padding="@dimen/grid_0_5"
            android:layout_width="@dimen/grid_5"
            android:layout_height="@dimen/grid_5"
            android:scaleType="centerInside"
            app:layout_constraintBottom_toBottomOf="@+id/buttonClose"
            app:layout_constraintDimensionRatio="H, 1:1"
            app:layout_constraintStart_toEndOf="@+id/buttonClose"
            app:layout_constraintTop_toTopOf="@+id/buttonClose"
            app:shapeAppearanceOverlay="@style/ShapeAppearance.Superfone.RoundedImageView"/>
        <com.google.android.material.textview.MaterialTextView
            android:textAppearance="@style/TextAppearance.Superfone.Headline5"
            android:gravity="start"
            android:id="@+id/chatTitle"
            android:tag="binding_3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_1"
            android:maxLines="1"
            android:layout_marginEnd="@dimen/grid_0_5"
            app:layout_constraintEnd_toStartOf="@+id/placeCallButton"
            app:layout_constraintStart_toEndOf="@+id/ivContactProfilePic"
            app:layout_constraintTop_toTopOf="@+id/ivContactProfilePic"/>
        <com.google.android.material.textview.MaterialTextView
            android:textAppearance="@style/TextAppearance.Superfone.Headline6"
            android:textColor="?attr/colorOnSurface"
            android:id="@+id/tvContactNumber"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_0_5"
            android:text="Team Chat"
            app:layout_constraintEnd_toEndOf="@+id/chatTitle"
            app:layout_constraintStart_toStartOf="@+id/chatTitle"
            app:layout_constraintTop_toBottomOf="@+id/chatTitle"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/placeCallButton"
            android:tag="binding_4"
            android:layout_width="@dimen/grid_5"
            android:layout_height="0dp"
            android:tint="?attr/colorPrimary"
            app:contentPaddingBottom="@dimen/grid_1_5"
            app:contentPaddingStart="@dimen/grid_1_5"
            app:contentPaddingTop="@dimen/grid_1_5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/openWhatsappButton"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_call"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/openWhatsappButton"
            android:tag="binding_5"
            android:layout_width="@dimen/grid_5"
            android:layout_height="0dp"
            android:tint="@color/superfone_green"
            app:contentPaddingBottom="@dimen/grid_1_5"
            app:contentPaddingStart="@dimen/grid_1_5"
            app:contentPaddingTop="@dimen/grid_1_5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/membersAdded"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_whatsapp"/>
        <com.google.android.material.textview.MaterialTextView
            android:textAppearance="@style/TextAppearance.Superfone.Body4"
            android:textColor="?attr/colorOnPrimary"
            android:id="@+id/membersAdded"
            android:background="@color/superfone_purple"
            android:paddingTop="@dimen/grid_0_5"
            android:paddingBottom="@dimen/grid_0_5"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/grid_1"
            android:paddingEnd="@dimen/grid_1"
            android:layout_marginStart="@dimen/grid_1"
            android:layout_marginEnd="@dimen/grid_1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/chatAddMembers"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/chatAddMembers"
            android:tag="binding_6"
            android:paddingTop="@dimen/grid_1_5"
            android:paddingBottom="@dimen/grid_1_5"
            android:layout_width="@dimen/grid_5"
            android:layout_height="0dp"
            app:contentPaddingStart="@dimen/grid_1_5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/chatDoMore"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_plus"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/chatDoMore"
            android:paddingTop="@dimen/grid_1_5"
            android:paddingBottom="@dimen/grid_1_5"
            android:visibility="gone"
            android:layout_width="@dimen/grid_5"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_more"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/chatDetailTitleOnSelection"
        android:tag="binding_7"
        android:background="?attr/colorSurface"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chatDetailTitle">
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/buttonCloseOnSelection"
            android:layout_width="@dimen/grid_4"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_back_arrow"/>
        <com.google.android.material.textview.MaterialTextView
            android:textAppearance="@style/TextAppearance.Superfone.Headline5"
            android:gravity="start"
            android:id="@+id/chatTitleOnSelection"
            android:tag="binding_8"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:layout_marginEnd="@dimen/grid_0_5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/chatStarMessage"
            app:layout_constraintStart_toEndOf="@+id/buttonCloseOnSelection"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/chatMessageCopy"
            android:padding="@dimen/grid_1"
            android:visibility="gone"
            android:layout_width="@dimen/grid_5"
            android:layout_height="@dimen/grid_5"
            android:tint="@color/superfone_gray"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/chatMessageInfo"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_copy"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/chatMessageInfo"
            android:padding="@dimen/grid_1"
            android:visibility="gone"
            android:layout_width="@dimen/grid_5"
            android:layout_height="@dimen/grid_5"
            android:tint="@color/superfone_gray"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/chatStarMessage"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_info"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/chatStarMessage"
            android:padding="@dimen/grid_1"
            android:visibility="gone"
            android:layout_width="@dimen/grid_5"
            android:layout_height="@dimen/grid_5"
            android:tint="@color/yellow_700"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/chatDownloadFile"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_star"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/chatDownloadFile"
            android:padding="@dimen/grid_1"
            android:visibility="gone"
            android:layout_width="@dimen/grid_5"
            android:layout_height="@dimen/grid_5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/chatShareFile"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_download"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/chatShareFile"
            android:padding="@dimen/grid_1"
            android:visibility="gone"
            android:layout_width="@dimen/grid_5"
            android:layout_height="@dimen/grid_5"
            android:layout_marginEnd="@dimen/grid_2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_share"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <com.facebook.shimmer.ShimmerFrameLayout
        android:id="@+id/shimmerLayout"
        android:animationCache="true"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:animateLayoutChanges="true"
        app:layout_constraintBottom_toTopOf="@+id/layout_send_message"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chatDetailTitleOnSelection"
        app:shimmer_repeat_mode="restart"
        app:shimmer_shape="radial">
        <LinearLayout
            android:orientation="vertical"
            android:tag="binding_9"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <include layout="@layout/content_shimmer_chat_my"/>
            <include layout="@layout/content_shimmer_chat_other"/>
            <include layout="@layout/content_shimmer_chat_my"/>
            <include layout="@layout/content_shimmer_chat_other"/>
            <include layout="@layout/content_shimmer_chat_my"/>
            <include layout="@layout/content_shimmer_chat_other"/>
            <include layout="@layout/content_shimmer_chat_my"/>
            <include layout="@layout/content_shimmer_chat_other"/>
            <include layout="@layout/content_shimmer_chat_my"/>
            <include layout="@layout/content_shimmer_chat_other"/>
            <include layout="@layout/content_shimmer_chat_my"/>
        </LinearLayout>
    </com.facebook.shimmer.ShimmerFrameLayout>
    <RelativeLayout
        android:id="@+id/newUnreadMessageBlock"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chatDetailTitleOnSelection">
        <com.google.android.material.chip.Chip
            android:textStyle="bold"
            android:textColor="?attr/colorOnPrimary"
            android:id="@+id/newUnreadMessageChip"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="5 New Messages"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:backgroundTint="?attr/colorPrimary"/>
    </RelativeLayout>
    <androidx.recyclerview.widget.RecyclerView
        android:orientation="vertical"
        android:id="@+id/rvChatMessagesList"
        android:background="@color/backgroundTasks20"
        android:scrollbars="vertical"
        android:clipToPadding="false"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:nestedScrollingEnabled="true"
        app:layoutManager="ai.salesmax.view.WrapContentLinearLayoutManager"
        app:layout_constraintBottom_toTopOf="@+id/layout_send_message"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chatDetailTitleOnSelection"
        app:reverseLayout="true"
        app:spanCount="1"/>
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/layout_send_message"
        android:background="@color/backgroundTasks20"
        android:paddingBottom="@dimen/grid_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">
        <com.google.android.material.card.MaterialCardView
            android:id="@+id/autoCallExplanation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/grid_0_5"
            android:layout_marginBottom="@dimen/grid_1"
            android:layout_weight="1"
            android:layout_marginStart="@dimen/grid_1"
            android:layout_marginEnd="@dimen/grid_1"
            android:backgroundTint="@color/superfone_white"
            app:cardCornerRadius="@dimen/grid_3"
            app:cardElevation="1dp"
            app:cardMaxElevation="1dp"
            app:layout_constraintTop_toTopOf="parent"
            app:materialThemeOverlay="@style/ThemeOverlay.Superfone.ElevationOverlayDisabled"
            style="?attr/materialCardViewElevatedStyle">
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/layout_message_with_reply"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textStyle="bold"
                    android:textColor="@color/superfone_red"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:id="@+id/textNewConversationAlert"
                    android:tag="binding_10"
                    android:padding="8dp"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_marginHorizontal="8dp"
                    tools.visibility="visible"/>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/reply_layout"
                    android:background="?attr/colorSurface"
                    android:padding="@dimen/grid_1"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">
                    <View
                        android:id="@+id/image_line"
                        android:background="#732d77"
                        android:layout_width="2dp"
                        android:layout_height="0dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/textQuotedMessageSentBy"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginHorizontal="8dp"
                        app:layout_constraintStart_toStartOf="@+id/image_line"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <com.google.android.material.textview.MaterialTextView
                        android:textSize="12sp"
                        android:textColor="?attr/colorOnSurface"
                        android:ellipsize="end"
                        android:id="@+id/textQuotedMessage"
                        android:padding="8dp"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:maxLines="3"
                        android:layout_marginHorizontal="8dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/cancelButton"
                        app:layout_constraintStart_toStartOf="@+id/image_line"
                        app:layout_constraintTop_toBottomOf="@+id/textQuotedMessageSentBy"/>
                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/cancelButton"
                        android:background="@drawable/ic_delete_engagement"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginBottom="8dp"
                        android:layout_marginHorizontal="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@+id/layout_message_icons"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent">
                    <com.google.android.material.imageview.ShapeableImageView
                        android:layout_gravity="center"
                        android:id="@+id/emoticonButton"
                        android:background="?attr/colorSurface"
                        android:visibility="gone"
                        android:layout_width="@dimen/grid_4"
                        android:layout_height="@dimen/grid_4"
                        android:tint="@color/superfone_gray"
                        android:layout_marginStart="@dimen/grid_1"
                        app:contentPadding="@dimen/grid_0_5"
                        app:shapeAppearanceOverlay="@style/ShapeAppearance.Superfone.RoundedImageView"
                        app:srcCompat="@drawable/ic_emoticon"/>
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/messageTextLayout"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        app:boxBackgroundMode="none"
                        app:boxCornerRadiusBottomEnd="0dp"
                        app:boxCornerRadiusBottomStart="0dp"
                        app:boxCornerRadiusTopEnd="0dp"
                        app:boxCornerRadiusTopStart="0dp"
                        app:hintAnimationEnabled="false"
                        app:hintEnabled="false"
                        style="@style/Superfone.Input.WithoutLine">
                        <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                            android:textAppearance="@style/TextAppearance.Superfone.Body1"
                            android:textColor="?attr/colorOnSurface"
                            android:id="@+id/taskCommentEditText"
                            android:paddingTop="16dp"
                            android:paddingBottom="16dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Message"
                            android:layout_weight="1"
                            android:inputType="textMultiLine"
                            android:imeOptions="actionDone"
                            android:paddingStart="@dimen/grid_1"
                            android:layout_marginStart="@dimen/grid_1"
                            android:paddingVertical="16dp"
                            style="@style/Widget.AppCompat.Light.AutoCompleteTextView"/>
                    </com.google.android.material.textfield.TextInputLayout>
                    <com.google.android.material.imageview.ShapeableImageView
                        android:layout_gravity="center"
                        android:id="@+id/cameraButton"
                        android:background="?attr/colorSurface"
                        android:layout_width="@dimen/grid_4"
                        android:layout_height="@dimen/grid_4"
                        android:tint="@color/superfone_gray"
                        android:layout_marginEnd="@dimen/grid_1"
                        app:contentPadding="@dimen/grid_0_5"
                        app:shapeAppearanceOverlay="@style/ShapeAppearance.Superfone.RoundedImageView"
                        app:srcCompat="@drawable/ic_camera"/>
                    <com.google.android.material.imageview.ShapeableImageView
                        android:layout_gravity="center"
                        android:id="@+id/sendTemplateButton"
                        android:background="?attr/colorSurface"
                        android:visibility="gone"
                        android:layout_width="@dimen/grid_4"
                        android:layout_height="@dimen/grid_4"
                        android:tint="@color/superfone_gray"
                        android:layout_marginEnd="@dimen/grid_1"
                        app:contentPadding="@dimen/grid_0_5"
                        app:shapeAppearanceOverlay="@style/ShapeAppearance.Superfone.RoundedImageView"
                        app:srcCompat="@drawable/ic_message_text"/>
                    <com.google.android.material.imageview.ShapeableImageView
                        android:layout_gravity="center"
                        android:id="@+id/attachButton"
                        android:background="?attr/colorSurface"
                        android:layout_width="@dimen/grid_4"
                        android:layout_height="@dimen/grid_4"
                        android:tint="@color/superfone_gray"
                        android:layout_marginEnd="@dimen/grid_1"
                        app:contentPadding="@dimen/grid_0_5"
                        app:shapeAppearanceOverlay="@style/ShapeAppearance.Superfone.RoundedImageView"
                        app:srcCompat="@drawable/ic_attach"/>
                </LinearLayout>
            </LinearLayout>
        </com.google.android.material.card.MaterialCardView>
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_gravity="center"
            android:id="@+id/sendMessageButton"
            android:background="@color/progressColor4"
            android:layout_width="@dimen/grid_5"
            android:layout_height="@dimen/grid_5"
            android:tint="@color/superfone_white"
            android:layout_marginEnd="@dimen/grid_1"
            app:contentPadding="@dimen/grid_1_5"
            app:shapeAppearanceOverlay="@style/ShapeAppearance.Superfone.RoundedImageView"
            app:srcCompat="@drawable/ic_microphone"/>
    </LinearLayout>
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_gravity="end|bottom"
        android:id="@+id/fabAddUser"
        android:visibility="visible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:backgroundTint="@color/superfone_white"
        app:layout_constraintBottom_toTopOf="@+id/layout_send_message"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_scroll_down"
        app:tint="@color/superfone_blue"
        style="?attr/floatingActionButtonSmallStyle"/>
    <FrameLayout
        android:id="@+id/imageOverlayContainer"
        android:background="@color/superfone_black_70"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:elevation="10dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.ortiz.touchview.TouchImageView
                android:id="@+id/imageViewer"
                android:tag="binding_11"
                android:background="@color/superfone_black_70"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <ImageView
            android:layout_gravity="end|top"
            android:id="@+id/closeButton"
            android:padding="@dimen/grid_0_5"
            android:layout_width="@dimen/grid_5"
            android:layout_height="@dimen/grid_5"
            android:src="@drawable/ic_cross"
            app:tint="@color/superfone_white"/>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
